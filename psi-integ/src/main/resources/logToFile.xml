<?xml version="1.0" encoding="UTF-8"?>
<included>
    <appender name="sitelogfile"
              class="ch.qos.logback.core.FileAppender">
        <file>logs/application.log</file>
        <encoder>
            <!--<pattern>%.-1level|%-30.30logger{0}|%msg%n</pattern>-->
            <pattern>%date|%.-1level|MVC|%-30.30logger{0}|%msg%n</pattern>
        </encoder>
        <append>false</append>
    </appender>

    <logger name="org.muhia.app.phi.orm" level="info"/>
    <logger name="org.springframework.ws.client.MessageTracing" level="trace"/>
    <logger name="org.muhia.app.psi.infra" level="info"/>
    <logger name="org.muhia.app.psi.integ" level="info"/>
    <logger name="org.muhia.app.psi.config" level="info"/>
    <logger name="org.springframework" level="info"/>
    <logger name="org.freemarker" level="info"/>

</included>